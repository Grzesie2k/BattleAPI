{
  "name": "grzesie2k/battleapi",
  "description": "BattleAPI is a simple, unofficial Battlelog API in PHP",
  "homepage": "https://github.com/Grzesie2k/BattleAPI",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Grzegorz Kielak",
      "homepage": "http://grzesie2k.github.io"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.5.*"
  },
  "autoload": {
    "psr-4": {
      "BattleAPI\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BattleAPI\\Tests\\": "tests/"
    }
  }
}
